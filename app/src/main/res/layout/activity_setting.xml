<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context=".SettingActivity">


    <androidx.appcompat.widget.Toolbar
        app:title="Settings"
        app:titleMarginStart="145dp"
        app:titleTextColor="#ffffff"
        android:id="@+id/settingToolbar"
        android:background="#A48D61"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


    <ImageView
        android:onClick="back"
        android:layout_marginTop="13dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/back"
        android:layout_width="30dp"
        android:layout_height="30dp"/>


    <LinearLayout
        
        android:id="@+id/about"
        android:layout_below="@id/settingToolbar"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="65sp">


        <ImageView
            android:onClick="about_next"
            android:src="@drawable/about"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </LinearLayout>


    <LinearLayout
        android:id="@+id/terms"
        android:layout_below="@id/about"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="65sp">



        <ImageView
            android:onClick="terms_next"
            android:src="@drawable/terms"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </LinearLayout>


    <LinearLayout
        android:id="@+id/privacy"
        android:layout_below="@id/terms"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="65sp">



        <ImageView
            android:onClick="privacy_next"
            android:src="@drawable/privacy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </LinearLayout>



    <LinearLayout
        android:id="@+id/share"
        android:layout_below="@id/privacy"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="65sp">



        <ImageView
            android:onClick="share_next"
            android:src="@drawable/share"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </LinearLayout>


    <LinearLayout
        android:id="@+id/more"
        android:layout_below="@id/share"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="65sp">



        <ImageView
            android:onClick="more_next"
            android:src="@drawable/more"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </LinearLayout>



    <LinearLayout
        android:id="@+id/contact"
        android:layout_below="@id/more"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="65sp">



        <ImageView
            android:onClick="contact_next"
            android:src="@drawable/contact"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </LinearLayout>


    <LinearLayout
        android:layout_below="@id/contact"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="65sp">



        <ImageView
            android:onClick="rate_next"
            android:src="@drawable/rate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </LinearLayout>




</RelativeLayout>